{
  "name": "voice-agent",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "node --test --import tsx",
    "test:watch": "node --test --watch --import tsx",
    "dev": "tsx watch src/agent.ts dev",
    "download-files": "pnpm run build && node dist/agent.js download-files",
    "start": "node dist/agent.js start"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.11",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@livekit/agents": "^1.0.11",
    "@livekit/agents-plugin-deepgram": "^1.0.11",
    "@livekit/agents-plugin-elevenlabs": "^1.0.11",
    "@livekit/agents-plugin-openai": "^1.0.11",
    "@livekit/agents-plugin-livekit": "^1.0.11",
    "@livekit/agents-plugin-silero": "^1.0.11",
    "@livekit/noise-cancellation-node": "^0.1.9",
    "dotenv": "^17.2.3"
  }
}
